{
  "name": "be-sales-tracking",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev:env && nodemon --exec npm run babel-node -- ./src/bin/www.js -x",
    "babel-node": "babel-node",
    "dev:env": "set NODE_ENV=development",
    "clean": "rm -rf dist",
    "build": "set NODE_ENV=production && npm run clean && babel ./src --out-dir dist --copy-files",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seeder": "npx sequelize-cli db:seed:all",
    "seeder:undo": "npx sequelize-cli db:seed:undo:all",
    "build-server": "babel -d ./dist ./src -s",
    "prod": "npm run build-server && npx pm2 start ecosystem.config.js"
  },
  "author": "nuel",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.20.7",
    "axios": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "core-js": "^3.27.2",
    "cors": "^2.8.5",
    "crypto-js": "4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mysql2": "^3.0.1",
    "nodemailer": "^6.9.1",
    "nodemailer-express-handlebars": "^6.0.0",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pdfkit-table": "^0.1.99",
    "sequelize": "^6.28.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "sequelize-cli": "^6.6.0"
  }
}
